{
    "name": "immortal/routing",
    "description": "The Immortal Routing package.",
    "license": "MIT",
    "homepage": "https://zgutu.com",
    "support": {
        "issues": "https://github.com/zgutu/framework/issues",
        "source": "https://github.com/zgutu/framework"
    },
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@zgutu.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "immortal/container": "5.3.*",
        "immortal/contracts": "5.3.*",
        "immortal/http": "5.3.*",
        "immortal/pipeline": "5.3.*",
        "immortal/session": "5.3.*",
        "immortal/support": "5.3.*",
        "symfony/debug": "3.1.*",
        "symfony/http-foundation": "3.1.*",
        "symfony/http-kernel": "3.1.*",
        "symfony/routing": "3.1.*"
    },
    "autoload": {
        "psr-4": {
            "Immortal\\Routing\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.3-dev"
        }
    },
    "suggest": {
        "immortal/console": "Required to use the make commands (5.3.*).",
        "symfony/psr-http-message-bridge": "Required to psr7 bridging features (0.2.*)."
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
