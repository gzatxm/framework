{
    "name": "immortal/console",
    "description": "The Immortal Console package.",
    "license": "MIT",
    "homepage": "https://zgutu.com",
    "support": {
        "issues": "https://github.com/zgutu/framework/issues",
        "source": "https://github.com/zgutu/framework"
    },
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@zgutu.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "immortal/contracts": "5.3.*",
        "immortal/support": "5.3.*",
        "nesbot/carbon": "~1.20",
        "symfony/console": "3.1.*"
    },
    "autoload": {
        "psr-4": {
            "Immortal\\Console\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.3-dev"
        }
    },
    "suggest": {
        "guzzlehttp/guzzle": "Required to use the ping methods on schedules (~5.3|~6.0).",
        "mtdowling/cron-expression": "Required to use scheduling component (~1.0).",
        "symfony/process": "Required to use scheduling component (3.1.*)."
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
