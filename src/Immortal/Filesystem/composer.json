{
    "name": "immortal/filesystem",
    "description": "The Immortal Filesystem package.",
    "license": "MIT",
    "homepage": "https://zgutu.com",
    "support": {
        "issues": "https://github.com/zgutu/framework/issues",
        "source": "https://github.com/zgutu/framework"
    },
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@zgutu.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "immortal/contracts": "5.3.*",
        "immortal/support": "5.3.*",
        "symfony/finder": "3.1.*"
    },
    "autoload": {
        "psr-4": {
            "Immortal\\Filesystem\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.3-dev"
        }
    },
    "suggest": {
        "league/flysystem": "Required to use the Flysystem local and FTP drivers (~1.0).",
        "league/flysystem-aws-s3-v3": "Required to use the Flysystem S3 driver (~1.0).",
        "league/flysystem-rackspace": "Required to use the Flysystem Rackspace driver (~1.0)."
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
