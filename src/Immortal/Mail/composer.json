{
    "name": "immortal/mail",
    "description": "The Immortal Mail package.",
    "license": "MIT",
    "homepage": "https://zgutu.com",
    "support": {
        "issues": "https://github.com/zgutu/framework/issues",
        "source": "https://github.com/zgutu/framework"
    },
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@zgutu.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "immortal/container": "5.3.*",
        "immortal/contracts": "5.3.*",
        "immortal/support": "5.3.*",
        "psr/log": "~1.0",
        "swiftmailer/swiftmailer": "~5.4"
    },
    "autoload": {
        "psr-4": {
            "Immortal\\Mail\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.3-dev"
        }
    },
    "suggest": {
        "aws/aws-sdk-php": "Required to use the SES mail driver (~3.0).",
        "guzzlehttp/guzzle": "Required to use the Mailgun and Mandrill mail drivers (~5.3|~6.0).",
        "jeremeamia/superclosure": "Required to be able to serialize closures (~2.0)."
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
