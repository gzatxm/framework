{
    "name": "immortal/queue",
    "description": "The Immortal Queue package.",
    "license": "MIT",
    "homepage": "https://zgutu.com",
    "support": {
        "issues": "https://github.com/zgutu/framework/issues",
        "source": "https://github.com/zgutu/framework"
    },
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@zgutu.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "immortal/console": "5.3.*",
        "immortal/container": "5.3.*",
        "immortal/contracts": "5.3.*",
        "immortal/support": "5.3.*",
        "nesbot/carbon": "~1.20",
        "symfony/debug": "3.1.*",
        "symfony/process": "3.1.*"
    },
    "autoload": {
        "psr-4": {
            "Immortal\\Queue\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.3-dev"
        }
    },
    "suggest": {
        "aws/aws-sdk-php": "Required to use the SQS queue driver (~3.0).",
        "immortal/redis": "Required to use the Redis queue driver (5.3.*).",
        "pda/pheanstalk": "Required to use the Beanstalk queue driver (~3.0)."
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
